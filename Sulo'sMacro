-- Sound system
local function createSound(soundId, volume, pitch)
    volume = volume or 0.5
    pitch = pitch or 1
    
    local sound = Instance.new("Sound")
    sound.SoundId = soundId
    sound.Volume = volume
    sound.Pitch = pitch
    sound.Parent = SoundService
    
    return sound
end

local function playSound(soundId, volume, pitch)
    local sound = createSound(soundId, volume, pitch)
    sound:Play()
    
    sound.Ended:Connect(function()
        sound:Destroy()
    end)
    
    -- Backup destroy in case sound doesn't end properly
    spawn(function()
        wait(10)
        if sound.Parent then
            sound:Destroy()
        end
    end)
end

-- Clean notification system that actually works (now with sound)
local function createNotification(title, message, color, duration, soundType)
    duration = duration or 3
    soundType = soundType or "notification"
    
    -- Play notification sound
    if soundType == "respawn" then
        playSound(SOUND_IDS.characterRespawn, 0.6, 1)
    else
        playSound(SOUND_IDS.notification, 0.4, 1)
    end
    
    local notification = Instance.new("Frame")
    notification.Size = UDim2.new(0, 300, 0, 70)
    notification.Position = UDim2.new(1, 20, 0, 100) -- Start off screen
    notification.BackgroundColor3 = color or Color3.fromRGB(50, 50, 70)
    notification.BorderSizePixel = 0
    notification.Parent = gui
    notification.ZIndex = 1
    
    local corner = Instance.new("UICorner")
    corner.CornerRadius = UDim.new(0, 10)
    corner.Parent = notification
    
    local stroke = Instance.new("UIStroke")
    stroke.Color = Color3.fromRGB(255, 255, 255)
    stroke.Thickness = 1
    stroke.Transparency = 0.7
    stroke.Parent = notification
    
    -- Title text
    local titleLabel = Instance.new("TextLabel")
    titleLabel.Size = UDim2.new(1, -20, 0, 25)
    titleLabel.Position = UDim2.new(0, 10, 0, 5)
    titleLabel.BackgroundTransparency = 1
    titleLabel.Text = title
    titleLabel.TextColor3 = Color3.new(1, 1, 1)
    titleLabel.TextTransparency = 0
    titleLabel.TextSize = 14
    titleLabel.Font = Enum.Font.GothamBold
    titleLabel.TextXAlignment = Enum.TextXAlignment.Left
    titleLabel.ZIndex = 1
    titleLabel.Parent = notification
    
    -- Message text
    local messageLabel = Instance.new("TextLabel")
    messageLabel.Size = UDim2.new(1, -20, 0, 35)
    messageLabel.Position = UDim2.new(0, 10, 0, 25)
    messageLabel.BackgroundTransparency = 1
    messageLabel.Text = message
    messageLabel.TextColor3 = Color3.fromRGB(220, 220, 220)
    messageLabel.TextTransparency = 0
    messageLabel.TextSize = 11
    messageLabel.Font = Enum.Font.Gotham
    messageLabel.TextXAlignment = Enum.TextXAlignment.Left
    messageLabel.TextYAlignment = Enum.TextYAlignment.Top
    messageLabel.TextWrapped = true
    messageLabel.ZIndex = 1
    messageLabel.Parent = notification
    
    -- Slide in animation
    local slideIn = TweenService:Create(notification,
        TweenInfo.new(0.4, Enum.EasingStyle.Quart, Enum.EasingDirection.Out),
        {Position = UDim2.new(1, -320, 0, 100)}
    )
    slideIn:Play()
    
    -- Auto dismiss
    spawn(function()
        wait(duration)
        
        local slideOut = TweenService:Create(notification,
            TweenInfo.new(0.3, Enum.EasingStyle.Quart, Enum.EasingDirection.In),
            {Position = UDim2.new(1, 20, 0, 100), BackgroundTransparency = 1}
        )
        
        local fadeText = TweenService:Create(titleLabel,
            TweenInfo.new(0.3, Enum.EasingStyle.Quad),
            {TextTransparency = 1}
        )
        
        local fadeMessage = TweenService:Create(messageLabel,
            TweenInfo.new(0.3, Enum.EasingStyle.Quad),
            {TextTransparency = 1}
        )
        
        slideOut:Play()
        fadeText:Play()
        fadeMessage:Play()
        
        slideOut.Completed:Connect(function()
            notification:Destroy()
        end)
    end)
    
    return notification
end

-- Rain effect system (restored)
local function createRainDrop(parent)
    local drop = Instance.new("Frame")
    drop.Size = UDim2.new(0, 2, 0, math.random(10, 20))
    drop.Position = UDim2.new(0, math.random(0, parent.AbsoluteSize.X), 0, -25)
    drop.BackgroundColor3 = Color3.fromRGB(100, 150, 255)
    drop.BackgroundTransparency = 0.2
    drop.BorderSizePixel = 0
    drop.Parent = parent
    
    local corner = Instance.new("UICorner")
    corner.CornerRadius = UDim.new(1, 0)
    corner.Parent = drop
    
    -- Make it fall
    local fallTween = TweenService:Create(drop,
        TweenInfo.new(math.random(15, 25) / 10, Enum.EasingStyle.Linear),
        {Position = UDim2.new(0, drop.Position.X.Offset, 0, parent.AbsoluteSize.Y + 25)}
    )
    
    fallTween:Play()
    fallTween.Completed:Connect(function()
        drop:Destroy()
    end)
end

