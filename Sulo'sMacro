    supportButton.Parent = supportFrame
    
    local buttonCorner = Instance.new("UICorner")
    buttonCorner.CornerRadius = UDim.new(0, 6)
    buttonCorner.Parent = supportButton
    
    -- Support button functionality (with sound)
    supportButton.MouseButton1Click:Connect(function()
        -- Play button click sound
        playSound(SOUND_IDS.buttonClick, 0.7, 1)
        
        setclipboard(DONATE_LINK)
        
        -- Button animation
        local scaleUp = TweenService:Create(supportButton,
            TweenInfo.new(0.1, Enum.EasingStyle.Back),
            {Size = UDim2.new(0, 65, 0, 28)}
        )
        scaleUp:Play()
        
        scaleUp.Completed:Connect(function()
            local scaleDown = TweenService:Create(supportButton,
                TweenInfo.new(0.2, Enum.EasingStyle.Back),
                {Size = UDim2.new(0, 60, 0, 25)}
            )
            scaleDown:Play()
        end)
        
        createNotification(
            "ðŸ’– Gamepass link Copied Suscsesfully!",
            "ðŸ’– Gamepass link copied to clipboard. Paste it in you're browser and buy the support Gamepass    âš  NOT A HACK! âš ",
            Color3.fromRGB(50, 200, 50),
            3,
            "notification"
        )
    end)
    
    -- Button hover sound effect
    supportButton.MouseEnter:Connect(function()
        playSound(SOUND_IDS.buttonClick, 0.3, 1.5) -- Softer, higher pitch for hover
    end)
    
    -- Fixed drag functionality
    local dragStart = nil
    local startPos = nil
    local dragging = false
    
    mainFrame.InputBegan:Connect(function(input)
        if input.UserInputType == Enum.UserInputType.MouseButton1 then
            dragging = true
            dragStart = input.Position
            startPos = mainFrame.Position
            
            -- Visual feedback - make drag handle green
            local dragGreen = TweenService:Create(dragHandle,
                TweenInfo.new(0.1, Enum.EasingStyle.Quad),
                {BackgroundColor3 = Color3.fromRGB(100, 255, 100), BackgroundTransparency = 0.2}
            )
            dragGreen:Play()
            
            local scaleUp = TweenService:Create(mainFrame,
                TweenInfo.new(0.1, Enum.EasingStyle.Quad),
                {Size = UDim2.new(0, 325, 0, 145)}
            )
            scaleUp:Play()
            
            local connection
            connection = UserInputService.InputChanged:Connect(function(moveInput)
                if moveInput.UserInputType == Enum.UserInputType.MouseMovement and dragging then
                    local delta = moveInput.Position - dragStart
                    mainFrame.Position = UDim2.new(
                        startPos.X.Scale,
                        startPos.X.Offset + delta.X,
                        startPos.Y.Scale,
                        startPos.Y.Offset + delta.Y
                    )
                end
            end)
            
            local endConnection
            endConnection = UserInputService.InputEnded:Connect(function(endInput)
                if endInput.UserInputType == Enum.UserInputType.MouseButton1 then
                    dragging = false
                    connection:Disconnect()
                    endConnection:Disconnect()
                    
                    -- Return drag handle to normal color
                    local dragNormal = TweenService:Create(dragHandle,
                        TweenInfo.new(0.2, Enum.EasingStyle.Quad),
                        {BackgroundColor3 = Color3.fromRGB(150, 150, 200), BackgroundTransparency = 0.5}
                    )
                    dragNormal:Play()
                    
                    local scaleDown = TweenService:Create(mainFrame,
                        TweenInfo.new(0.2, Enum.EasingStyle.Quad),
                        {Size = UDim2.new(0, 320, 0, 140)}
                    )
                    scaleDown:Play()
                end
            end)
        end
    end)
end

-- Speed control functions (same as before but cleaner)
local function setSpeedUltimate(humanoid, targetSpeed)
    if not humanoid or not humanoid.Parent then return end
    
    pcall(function() humanoid.WalkSpeed = targetSpeed end)
    
    local character = humanoid.Parent
    if character then
        pcall(function()
            character:SetAttribute("WalkSpeed", targetSpeed)
            character:SetAttribute("Speed", targetSpeed)
        end)
        
        for _, child in pairs(character:GetChildren()) do
            pcall(function()
                if child.Name:lower():find("speed") and (child:IsA("NumberValue") or child:IsA("IntValue")) then
                    child.Value = targetSpeed
                end
            end)
        end
    end
    
    pcall(function()
        for _, remote in pairs(ReplicatedStorage:GetChildren()) do
            if remote:IsA("RemoteEvent") and remote.Name:lower():find("speed") then
                remote:FireServer(targetSpeed)
            end
        end
    end)
end

local function getCurrentHumanoid()
    if player.Character and player.Character:FindFirstChild("Humanoid") then
        return player.Character.Humanoid
    end
    return nil
end

local function startSpeedEnforcement()
    local humanoid = getCurrentHumanoid()
    if not humanoid then return end
    
    currentHumanoid = humanoid
    
    for _, connection in pairs(connections) do
        if connection then connection:Disconnect() end
    end
    connections = {}
    
    connections[#connections + 1] = RunService.Heartbeat:Connect(function()
        if speedLocked and currentHumanoid and currentHumanoid.Parent then
            setSpeedUltimate(currentHumanoid, SPEED_BOOST)
        end
    end)
    
    connections[#connections + 1] = currentHumanoid:GetPropertyChangedSignal("WalkSpeed"):Connect(function()
        if speedLocked and currentHumanoid and currentHumanoid.Parent then
            if currentHumanoid.WalkSpeed ~= SPEED_BOOST then
                setSpeedUltimate(currentHumanoid, SPEED_BOOST)
            end
        end
    end)
end

local function stopSpeedEnforcement()
    for _, connection in pairs(connections) do
        if connection then connection:Disconnect() end
    end
    connections = {}
end

local function updateStatus()
    if not statusLabel then return end
    
    if speedLocked then
        statusLabel.Text = "ðŸ”’ Status: Fake Macro ACTIVE âš¡"
        
        local colorTween = TweenService:Create(statusLabel,
            TweenInfo.new(0.3, Enum.EasingStyle.Quad),
            {TextColor3 = Color3.fromRGB(100, 255, 150)}
        )
        colorTween:Play()
        
        if statusStroke then
            local strokeTween = TweenService:Create(statusStroke,
                TweenInfo.new(0.3, Enum.EasingStyle.Quad),
                {Color = Color3.fromRGB(50, 255, 100)}
            )
            strokeTween:Play()
        end
        
    else
        statusLabel.Text = "ðŸ”“ Status: Ready"
        
        local colorTween = TweenService:Create(statusLabel,
            TweenInfo.new(0.3, Enum.EasingStyle.Quad),
            {TextColor3 = Color3.fromRGB(255, 120, 120)}
        )
        colorTween:Play()
        
        if statusStroke then
            local strokeTween = TweenService:Create(statusStroke,
                TweenInfo.new(0.3, Enum.EasingStyle.Quad),
                {Color = Color3.fromRGB(255, 100, 100)}
            )
            strokeTween:Play()
        end
    end
end

local function toggleSpeed()
    speedLocked = not speedLocked
    
    if speedLocked then
        createNotification(
            "âœ… Fake Macro Activated!",
            "SUPER speed enabled! Press C again to disable.",
            Color3.fromRGB(100, 50, 200),
            3,
            "notification"
        )
        
        startSpeedEnforcement()
        
    else
        createNotification(
            "âŒ Fake Macro Deactivated!",
            "Walkspeed will be changed instantly!",
            Color3.fromRGB(50, 100, 200),
            2,
            "notification"
        )
        
        stopSpeedEnforcement()
        
        local humanoid = getCurrentHumanoid()
        if humanoid then
            setSpeedUltimate(humanoid, DEFAULT_SPEED)
        end
    end
    
    updateStatus()
end

-- Input handling
UserInputService.InputBegan:Connect(function(input, gameProcessed)
    if gameProcessed then return end
    
    if input.KeyCode == Enum.KeyCode.C then
        toggleSpeed()
    end
end)

-- Handle respawning (with special respawn sound)
player.CharacterAdded:Connect(function(character)
    local humanoid = character:WaitForChild("Humanoid")
    wait(1)
    
    if speedLocked then
        createNotification(
            "ðŸ”„ Character Respawned",
            "Macro restored automatically.",
            Color3.fromRGB(50, 150, 200),
            2,
            "respawn" -- Special sound for respawn
        )
        startSpeedEnforcement()
    end
    
    updateStatus()
end)

-- Initialize everything
print("ðŸš€ Initializing Sulo's Fixed Fake Macro...")

createWelcomeScreen()

spawn(function()
    wait(4)
    createMainGUI()
    updateStatus()
    
    wait(0.5)
    createNotification(
        "âœ… Macro Loaded Successfully!",
        "All systems ready. Press C to activate speed boost.",
        Color3.fromRGB(50, 200, 50),
        3,
        "notification"
    )
    
    print("âœ… Loaded everything for you bro - Sulo!")
    print("âš¡ Press C to toggle speed boost")
    print("ðŸ’– an little donation would really give me motivation for better stuff")
end)
