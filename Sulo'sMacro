local player = Players.LocalPlayer
local playerGui = player:WaitForChild("PlayerGui")

-- Configuration
local SPEED_BOOST = 300
local DEFAULT_SPEED = 16
local DONATE_LINK = "https://www.roblox.com/catalog/132268448125497/Support"

-- Sound IDs
local SOUND_IDS = {
    buttonClick = "rbxassetid://9042321568",
    notification = "rbxassetid://98777702363840",
    characterRespawn = "rbxassetid://140273873013760"
}

-- Variables
local speedLocked = false
