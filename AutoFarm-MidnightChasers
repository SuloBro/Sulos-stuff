local WAYPOINTS = {
	Vector3.new(-5900, -15, -4785),
	Vector3.new(-3166, -17, -5388),
    Vector3.new(-4832, -17, -4637),
    Vector3.new(-5051, -17, -5411),
	Vector3.new(-2906, -17, -5012),
    Vector3.new(-5180, -17, -3967),
	Vector3.new(-5962, -17, -4379),
    Vector3.new(-3887, -17, -5264),
    Vector3.new(-5870, -17, -4741)
}

local function createRoadSection(position, rotation)
	local roadModel = Instance.new("Model")
	roadModel.Name = "Road"
	
	local mainRoad = Instance.new("Part")
	mainRoad.Name = "Road"
	mainRoad.Size = Vector3.new(512, 1, 512)
	mainRoad.CFrame = CFrame.new(position) * CFrame.Angles(0, math.rad(rotation), 0)
	mainRoad.Anchored = true
	mainRoad.CanCollide = true
	mainRoad.Material = Enum.Material.Asphalt
	mainRoad.Color = Color3.fromRGB(60, 60, 60)
	mainRoad.Parent = roadModel
	
	for i = 1, 8 do
		local stripe = Instance.new("Part")
		stripe.Name = "Road_Mark"
		stripe.Size = Vector3.new(8, 0.1, 60)
		stripe.CFrame = mainRoad.CFrame * CFrame.new(-200 + (i * 50), 0.6, 0)
		stripe.Anchored = true
		stripe.CanCollide = false
		stripe.Material = Enum.Material.SmoothPlastic
		stripe.Color = Color3.fromRGB(255, 255, 255)
		stripe.Parent = roadModel
	end
	
	roadModel.Parent = workspace
	return roadModel
end

local function createFarmRoad()
	local minX, maxX = math.huge, -math.huge
	local minZ, maxZ = math.huge, -math.huge
	
	for _, waypoint in ipairs(WAYPOINTS) do
		minX = math.min(minX, waypoint.X)
		maxX = math.max(maxX, waypoint.X)
		minZ = math.min(minZ, waypoint.Z)
		maxZ = math.max(maxZ, waypoint.Z)
	end
	
	local centerX = (minX + maxX) / 2
	local centerZ = (minZ + maxZ) / 2
	local roadY = -17.5
	
	local sections = {
		{x = centerX, z = centerZ, rot = 0},
		{x = centerX - 512, z = centerZ, rot = 0},
		{x = centerX + 512, z = centerZ, rot = 0},
		{x = centerX, z = centerZ - 512, rot = 0},
		{x = centerX, z = centerZ + 512, rot = 0},
		{x = centerX - 512, z = centerZ - 512, rot = 0},
		{x = centerX + 512, z = centerZ - 512, rot = 0},
		{x = centerX - 512, z = centerZ + 512, rot = 0},
		{x = centerX + 512, z = centerZ + 512, rot = 0},
		{x = centerX - 1024, z = centerZ, rot = 0},
		{x = centerX + 1024, z = centerZ, rot = 0},
		{x = centerX, z = centerZ - 1024, rot = 0},
		{x = centerX, z = centerZ + 1024, rot = 0},
		{x = centerX - 1024, z = centerZ - 512, rot = 0},
		{x = centerX + 1024, z = centerZ - 512, rot = 0},
		{x = centerX - 1024, z = centerZ + 512, rot = 0},
		{x = centerX + 1024, z = centerZ + 512, rot = 0},
		{x = centerX - 512, z = centerZ - 1024, rot = 0},
		{x = centerX + 512, z = centerZ - 1024, rot = 0},
		{x = centerX - 512, z = centerZ + 1024, rot = 0},
		{x = centerX + 512, z = centerZ + 1024, rot = 0},
		{x = centerX - 1024, z = centerZ - 1024, rot = 0},
		{x = centerX + 1024, z = centerZ - 1024, rot = 0},
		{x = centerX - 1024, z = centerZ + 1024, rot = 0},
		{x = centerX + 1024, z = centerZ + 1024, rot = 0},
		{x = centerX - 1536, z = centerZ, rot = 0},
		{x = centerX + 1536, z = centerZ, rot = 0},
		{x = centerX, z = centerZ - 1536, rot = 0},
		{x = centerX, z = centerZ + 1536, rot = 0},
		{x = centerX - 1536, z = centerZ - 512, rot = 0},
		{x = centerX + 1536, z = centerZ - 512, rot = 0},
		{x = centerX - 1536, z = centerZ + 512, rot = 0},
		{x = centerX + 1536, z = centerZ + 512, rot = 0},
		{x = centerX - 512, z = centerZ - 1536, rot = 0},
		{x = centerX + 512, z = centerZ - 1536, rot = 0},
		{x = centerX - 512, z = centerZ + 1536, rot = 0},
		{x = centerX + 512, z = centerZ + 1536, rot = 0},
		{x = centerX - 1536, z = centerZ - 1024, rot = 0},
		{x = centerX + 1536, z = centerZ - 1024, rot = 0},
		{x = centerX - 1536, z = centerZ + 1024, rot = 0},
		{x = centerX + 1536, z = centerZ + 1024, rot = 0},
		{x = centerX - 1024, z = centerZ - 1536, rot = 0},
		{x = centerX + 1024, z = centerZ - 1536, rot = 0},
		{x = centerX - 1024, z = centerZ + 1536, rot = 0},
		{x = centerX + 1024, z = centerZ + 1536, rot = 0},
		{x = centerX - 1536, z = centerZ - 1536, rot = 0},
		{x = centerX + 1536, z = centerZ - 1536, rot = 0},
		{x = centerX - 1536, z = centerZ + 1536, rot = 0},
		{x = centerX + 1536, z = centerZ + 1536, rot = 0},
		{x = centerX - 2048, z = centerZ, rot = 0},
		{x = centerX + 2048, z = centerZ, rot = 0},
		{x = centerX, z = centerZ - 2048, rot = 0},
		{x = centerX, z = centerZ + 2048, rot = 0},
		{x = centerX - 2048, z = centerZ - 512, rot = 0},
		{x = centerX + 2048, z = centerZ - 512, rot = 0},
		{x = centerX - 2048, z = centerZ + 512, rot = 0},
		{x = centerX + 2048, z = centerZ + 512, rot = 0},
		{x = centerX - 512, z = centerZ - 2048, rot = 0},
		{x = centerX + 512, z = centerZ - 2048, rot = 0},
		{x = centerX - 512, z = centerZ + 2048, rot = 0},
		{x = centerX + 512, z = centerZ + 2048, rot = 0},
		{x = centerX - 2048, z = centerZ - 1024, rot = 0},
		{x = centerX + 2048, z = centerZ - 1024, rot = 0},
